{
	"version": 2,
	"builds": [
	  {
		"src": "api/index.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "dist/**",
		"use": "@vercel/static"
	  }
	],
	"routes": [
	  {
		"src": "/todos/(.*)",
		"dest": "/api/index.js"
	  },
	  {
		"src": "/todos",
		"dest": "/api/index.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "/dist/$1"
	  }
	]
  }
