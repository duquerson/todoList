{
	"version": 2,
	"builds": [
	  {
		"src": "api/index.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "package.json",
		"use": "@vercel/static-build"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "api/index.js"
	  },
	  {
		"src": "^/(?!api)(.*)",
		"dest": "dist/index.html"
	  }
	],
	"rewrites": [
    {
      "source": "^/(?!api)(.*)",
      "destination": "/index.html"
    }
]
}
